  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack --mode production && npm run copy-files",
    "copy-files": "copyfiles -u 1 public/**/* dist/",
    "dev:client": "webpack serve --mode development --open --host 0.0.0.0",
    "dev:server": "nodemon server.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" --kill-others",
    "test": "jest",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
